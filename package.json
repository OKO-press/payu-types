{
  "name": "@oko-press/payu-types",
  "version": "1.0.0",
  "packageManager": "yarn@4.10.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/OKO-press/payu-types.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/oko-press"
